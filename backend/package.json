{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "ts-node-dev --env-file=.env --respawn --transpile-only --ignore-watch src/swagger-schemas.ts src/server.ts",
    "dev-without-swagger": "SKIP_SWAGGER=1 ts-node-dev --env-file=.env --respawn --transpile-only --ignore-watch src/swagger-schemas.ts src/server.ts",
    "generate-erd": "ts-node src/scripts/generateErd.ts",
    "lint": "tsc --noEmit && eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json,md}\"",
    "format:check": "prettier --check .",
    "redoc": "redocly build-docs swagger-output.json --output ../docs/index.html",
    "prepare": "cd ../ && git rev-parse --is-inside-work-tree >/dev/null 2>&1 && git config core.hooksPath backend/.husky || true",
    "generate-swagger": "ts-node src/scripts/generateSwagger.ts",
    "generate-api-types": "npx swagger-typescript-api generate -p ./swagger-output.json -o ../frontend/src/api/types --extract-request-params --extract-request-body --extract-response-body --modular --axios --disable-strict-ssl --disable-throw-on-error",
    "clean-up-unused-images": "ts-node src/scripts/cleanUpUnusedImages.ts",
    "seed": "ts-node src/database/seed.ts",
    "seed:force": "ts-node src/database/seed.ts --force",
    "seed:verify": "ts-node src/database/verify-seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "overrides": {
    "glob": "^11.0.3",
    "rimraf": "^4.4.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.799.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.12.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "file-type": "^18.0.0",
    "multer": "^1.4.5-lts.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.13.1",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.7.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@redocly/cli": "^1.34.4",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/express-session": "^1.18.1",
    "@types/multer": "^1.4.12",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.7",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "sequelize-erd": "^1.3.1",
    "swagger-typescript-api": "^13.0.28",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript-eslint": "^8.17.0",
    "vitest": "^3.2.4"
  }
}
